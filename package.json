{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun build ./src/index.html --outdir=dist --minify && bun src/index.ts",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "bun src/index.ts",
    "watch": "bun build ./src/index.html --outdir=dist --watch"
  },
  "dependencies": {
    "@dicebear/collection": "^9.3.0",
    "@dicebear/core": "^9.3.1",
    "firebase": "^12.9.0",
    "phaser": "^3.90.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
